	//新流程
	public function webCountNew($stationid)
	{
		//连接数
		$sql = "SELECT COUNT(DISTINCT client) AS link FROM rha_aclog WHERE station = $stationid AND date > '2015-01-30' AND date < '".date('Y-m-d',time())."' GROUP BY date ORDER BY date DESC";
		$link = $this->Query($sql);
		//uv
		$sql = "SELECT
		date,
		GROUP_CONCAT(model, '/', num) AS uv
		FROM
		(
		SELECT
		date,
		model,
		SUM(dtime) AS num
		FROM
		rha_count
		WHERE
		stationid = $stationid
		AND date > '2015_01_30' AND date < '".date('Y_m_d',time())."'
		AND detail = 'uv'
		GROUP BY
		date,
		model
		) AS flow
		GROUP BY
		date
		ORDER BY
		date DESC";
		$uv = $this->Query($sql);
		//新注册
		$sql = "SELECT SUM(dtime) AS login FROM rha_count WHERE stationid = $stationid AND date > '2015_01_30' AND date < '".date('Y_m_d',time())."'AND model = 'register' AND action = 'login' AND detail = 'success' GROUP BY date ORDER BY date DESC";
		$login = $this->Query($sql);
		//train 下载
		$sql = "SELECT SUM(dtime) AS down FROM rha_count WHERE stationid = $stationid AND date > '2015_01_30' AND date < '".date('Y_m_d',time())."'AND model in ('game','movie','music','sindex') AND action = 'trainDown' AND detail = 'uv' GROUP BY date ORDER BY date DESC";
		$down = $this->Query($sql);
		
		for($i=0;$i<count($link);$i++)
		{
		$uv[$i]['link'] = $link[$i]['link'];
			$uv[$i]['login'] = $login[$i]['login'];
			$uv[$i]['down'] = $down[$i]['down'];
		}
		
		return $uv;
	}
