	//新流程
	public function webCountNew($stationid)
	{
		//连接数
		$sql = "SELECT date,COUNT(DISTINCT client) AS link FROM rha_aclog WHERE station = $stationid AND date > '2015-01-30' AND date < '".date('Y-m-d',time())."' GROUP BY date ORDER BY date DESC";
