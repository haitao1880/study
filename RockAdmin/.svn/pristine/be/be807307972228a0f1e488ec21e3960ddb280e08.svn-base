@charset "utf-8";
/* CSS Document */
*{
	padding:0;
	}
body {
	margin: 0;
	padding: 0;
	font-size: 12px;	
}
/*清除边距间距*/
div, ul, li, img, input, form, p, h1, h2, h3, h4, h5, span, b, font, tr, td, select {
	margin: 0;
	padding: 0;
}
/*清除列表符*/
ul, li {
	list-style: none;
}
/*清除图片边框*/
img {
	border: none;
	_border: 0;
}
/*链接样式*/
a {
	text-decoration: none;
}
/*清除浮动*/
.clear {
	clear: both;
}
input {
	font-family: 'Verdana';
}


.box_msg_topcenter {
	width: 100%;
	height: 25px;
	background-image:url(../images/box_msg_topcenter.jpg);
	background-repeat: repeat-x;
	overflow: hidden;
	line-height: 20px;
}
.box_msg_topleft {
	width: 100%;
	height: 100%;
	background-image:url(../images/box_msg_topleft.jpg);
	background-repeat: no-repeat;
	overflow: hidden;
}
.box_msg_title {
	float: left;
	padding: 3px 0 0 10px;
	color: white;
	font-weight: bold;
}
.box_msg_close {
	width: 18px;
	height: 18px;
	background-image:url(../images/box_msg_close.jpg);
	background-repeat: no-repeat;
	background-position: 0 100%;
	cursor: pointer;
	cursor: hand;
	float: right;
	padding: 3px 5px 0 0;
}
.box_msg_middlecenter {
	width: 100%;
	background-color: #e3eff7;
}
.box_msg_middleleft {
	width: 100%;
	background-image:url(../images/box_msg_middleleft.jpg);
	background-repeat: repeat-y;
}
.box_msg_middleright {
	width: 100%;
	background-image:url(../images/box_msg_middleright.jpg);
	background-repeat: repeat-y;
	background-position: 100% 0;
}
.box_msg_bottomcenter {
	width: 100%;
	height: 11px;
	background-image:url(../images/box_msg_bottomcenter.jpg);
	background-repeat: repeat-x;
	overflow: hidden;
	line-height: 11px;
}
.box_msg_bottomleft {
	width: 100%;
	height: 100%;
	background-image:url(../images/box_msg_bottomleft.jpg);
	background-repeat: no-repeat;
	overflow: hidden;
}
.box_msg_bottomright {
	width: 100%;
