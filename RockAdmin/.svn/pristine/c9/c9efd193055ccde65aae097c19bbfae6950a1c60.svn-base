@charset "utf-8";
/* CSS Document */

html, body {
	height:100%;
}
html {
	overflow-y:auto;
}

body {
	font: 12px arial, helvetica, sans-serif;
	background:#f1f1f1;
}
* {
	padding:0;
	margin:0;
	list-style:none;
	border:none;
}
#header {
	height:74px;
	width:100%;
	min-width:1380px;
	background:#323232;
	border-bottom:1px solid #191919;
	border-top:1px solid #656565;
}
#logo {
	float:left;
}
#top_nav {
	float:left;
}
#top_nav ul {
	float:left;
	width:1000px;
	height:74px;
}
#top_nav li {
	float:left;
	margin-left:90px;
	color:#FFF;
	position:relative;
	cursor:pointer;
}
#top_nav h4 {
	font-weight:normal;
	text-align:center;
	font-size:12px;
}
.point {
	background:url(../images/img008.jpg);
	width:8px;
	height:5px;
	position:absolute;
	z-index:10000;
	top:74px;
	left:50%;
	margin-left:-4px;
	visibility:hidden;
}
#main {
	width:100%;
	height:100%;
}
#user-bg {
	background:url(../images/img009.jpg) no-repeat;
	width:186px;
	height:74px;
	float:right;
}
#user-bg p {
	color:#989898;
	margin-left:25%;
}
#user-bg p:first-child {
	margin-top:25px;
}
#user-bg p a:link, #user-bg p a:visited {
	color:#989898;
	text-decoration:underline;
}
#user-bg p a:hover {
	color:#ffffff;
	text-decoration:none;
}
.aside {
	background:#417eb7;
	width:186px;
	height:100%;
	border-right:1px solid #005390;
	float:left;
	overflow-x: hidden;
}
.aside div {
	width:170px;
	height:38px;
	margin:0 auto;
	padding-top:15px;
}
.aside div h4 {
	background:url(../images/img010.jpg) no-repeat;
	padding-left:22px;
	line-height:19px;
	color:#FFF;
	width:auto;
	margin-left:10px;
	margin-bottom:7px;
}
.aside div h5 {
	padding-left:28px;
	line-height:19px;
	font-size:12px;
	color:#FFF;
	width:auto;
	margin-left:0px;
	margin-bottom:7px;
	cursor:pointer;
}
.fenline {
	background:url(../images/img011.jpg) repeat-x;
	height:2px;
	width:100%;
}
.fenline2 {
	height:2px;
	width:100%;
}
.aside ul {
	margin:0 auto;
	width:100%;
}
.aside ul li {
	padding-left:68px;
	height:30px;
	margin:8px auto;
	color:#bfeaf5;
	line-height:30px;
	cursor:pointer;
}
.market_li div {
	height:18px;
}
.market_li ul li {
	margin:0px;
	padding-left:48px;
	height:24px;
	line-height:24px;
}
.aside ul li:hover {
	background:#2d68a0;
	text-decoration:none;
}
.aside ul li a:link, .aside ul li a:visited {
	color:#bfeaf5;
	/*text-decoration:underline;*/
	text-decoration:none;
}
.aside ul li a:hover {
	color:#ffffff;
	text-decoration:none;
}
#context {
	width:auto;
	height:100%;
}
iframe {
	width:100%;
	height:100%;
}
