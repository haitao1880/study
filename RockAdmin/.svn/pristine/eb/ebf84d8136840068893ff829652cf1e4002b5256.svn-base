@charset "UTF-8";
/* CSS Document */

.search{
    
    background:#e9e9e9;
    padding:6px;
    width:100%;
    border-bottom:1px #d1d1d1 solid;
    
}

.search_point{
    
    padding:6px;
    
}

.button2{
    border:1px solid #881010;
    font: 12px arial, helvetica, sans-serif;
    padding:0 5px 0 5px;
    height:24px;
    color:white;
    background:#b74141;
    cursor: pointer;
}

.button2:hover{
    border:1px solid #b74141;
    background:#881010;
}

.input1{
    width:250px;
    height:22px;
    border:1px solid #ccc;
    font: 12px arial, helvetica, sans-serif;
    color:#696969;
    text-indent:2px;
}

.select2{
    width:122px;
    height:22px;
    border:1px solid #ccc;
    font: 12px arial, helvetica, sans-serif;
    color:#696969;
}

.Nchart{
    
    width:100%;
    height:100px;
    margin:10px;
    
}

.Nchart div.Nleft{
    
    width:10%;
    height:200px;
    float:left;
    
}

.Nchart div.Nleft ul li{
    
    height:22px;
    background:#B74141;
    line-height:22px;
    text-align:center;
    margin:1px;
    
}

.Nchart div.Nleft ul li a{
    color:white;
}

.Nchart div.Nright{
    
    width:89%;
    height:200px;
    float:left;
    
}