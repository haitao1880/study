			$this->SetDb('db-rht_train');
	
		$time = $date?date('d/M/Y',strtotime($date)):'';
	
			$grep = 'cat '.$this->file.' | grep "'.$time.'" | grep "GET /music/albummusic/" | cut -d " " -f 7| sort | uniq -c';
		exec($grep,$data);
	
			$res = array();
	
			foreach ($data as $key=>$var)
			{
			$arr = explode(' ', trim($var));
			$con = explode('/', $arr[1]);
