@charset "utf-8";
/* CSS Document */
*,html{
	padding:0;
	margin: 0;
	}
body {
	margin: 0;
	padding: 0;
	font-size: 12px;
	font-family:Verdana, Arial, Helvetica, sans-serif;
	background:#fff;
	/*word-break:break-all; /*自动换行处理*/
}
/*清除边距间距*/
div, ul, li, img, input, form, p, h1, h2, h3, h4, h5, span, b, font, tr, td, select {
	margin: 0;
	padding: 0;
}
/*清除列表符*/
ul, li {
	list-style: none; vertical-align:top;
}
/*清除图片边框*/
img {
	border: none;
	_border: 0;
}
/*链接样式*/
a {
	text-decoration: none;
}
/*清除浮动*/
.clear {
	clear: both;
}
input {
	border: none;
	font-family: 'Verdana';
}

